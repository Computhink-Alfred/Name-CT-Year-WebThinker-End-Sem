{
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vsliveshare.vsliveshare",
        "ritwickdey.liveserver",
        "vsls-contrib.gitdoc"
      ],
      "settings": {
        "gitdoc.enabled": true,
        "git.autofetch": true,
        "gitdoc.autoPullDelay": 5000
      }
    }
  },
  "postCreateCommand": "gitdoc enable"
}
